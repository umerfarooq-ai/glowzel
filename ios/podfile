# Uncomment this line to define a global platform for your project
platform :ios, '12.0'

# CocoaPods analytics sends network stats. Uncomment to disable.
# ENV['COCOAPODS_DISABLE_STATS'] = 'true'

project 'Runner', {
  'Debug' => :debug,
  'Profile' => :release,
  'Release' => :release,
}

# This line imports Flutterâ€™s CocoaPod helper functions
require File.expand_path('../Flutter/Flutter.podspec', __FILE__)

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  # Install all Flutter plugins
  flutter_install_all_ios_pods File.dirname(File.realpath(__FILE__))
end
